@{
    ViewBag.Title = "Home Page";
}

<html>
<body>

    <div class=" page-header">
        <div class=" btn-toolbar pull-right">
            <div class=" btn-group">
                <button type="button" class=" eButton">Edit Form</button>
            </div>
        </div>
        <h2>Order Form</h2>
    </div>
   

    

    <form>
        <div id="HomePage">
            <div class=" form-horizontal">
                Account Number: <input id="AccountNbr" type="text" name="AccountNbr">
                <button class=" btn">Edit Field</button>
            </div>

            <div style=" margin-top:5px"></div>

            <div class=" form-horizontal">
                Account Name 1: <input id="AccountName1" type="text" name="AccountName1">
                <button class=" btn">Edit Field</button>
            </div>

            <div style=" margin-top:5px"></div>

            <div class=" form-horizontal">
                Account Name 2: <input id="AccountName2" type="text" name="AccountName2">
                <button class=" btn">Edit Field</button>
            </div>

            <div style=" margin-top:5px"></div>

            <div class=" form-horizontal">
                First Name: <input id="Fname" type="text" name="Fname">
                <button class=" btn">Edit Field</button>
            </div>

            <div style=" margin-top:5px"></div>

            <div class=" form-horizontal">
                Last Name: <input id="Lname" type="text" name="Lname">
                <button class=" btn">Edit Field</button>
            </div>
            
            <div style=" margin-top:5px"></div>

            <div class=" form-horizontal">
                ID Number: <input id="IDNbr" type="text" name="IDNbr">
                <button class=" btn">Edit Field</button>
            </div>

            <div style=" margin-top:5px"></div>

            <div class=" form-horizontal">
                Holder Type: <input id="HType" type="text" name="HType" value="LA" maxlength="2">
                <button class=" btn">Edit Field</button>
            </div>

            <div style=" margin-top:5px"></div>

            <div class=" form-horizontal">
                Neutron: <input id="NT" type="text" name="NT" value="NT" maxlength="2">
                <button class=" btn">Edit Field</button>
            </div>

            <div style=" margin-top:5px"></div>

            <div class=" form-horizontal">
                Barcode: <input id="BarCode" type="text" name="BarCode" maxlength="12">
                <button class=" btn">Edit Field</button>
            </div>

            <div style=" margin-top:5px"></div>

            <div class=" form-horizontal">
                Wear Location: <input id="WLocation" type="text" name="WLocation" value="WB" maxlength="2">
                <button class=" btn">Edit Field</button>
            </div>

            <div style=" margin-top:5px"></div>

            <div class=" form-horizontal">
                Use Period Description: <input id="UPD" type="text" name="UPD" maxlength="11">
                <button class=" btn">Edit Field</button>
            </div>

            <div style=" margin-top:5px"></div>

            <div class=" form-horizontal">
                Series Name: <input id="SName" type="text" name="SName" maxlength="17">
                <button class=" btn">Edit Field</button>
            </div>

            <div style=" margin-top:5px"></div>

            <div class=" form-horizontal">
                Customer Key: <input id="CKey" type="text" name="CKey" maxlength="25">
                <button class=" btn">Edit Field</button>
            </div>

            <div style=" margin-top:5px"></div>

            <div class=" form-horizontal">
                Clip Type: <select>
                    <option value="SL"> SL- Slotted Clip</option>
                    <option value="AL">AL- Alligator Clip</option>
                </select>
                <button class=" btn">Edit Field</button>
            </div>

            <div style=" margin-top:5px"></div>

            <div class=" form-horizontal">
                Series Color: <select>
                    <option value="BL"> BL- Blue </option>
                    <option value="GR">GR - Green</option>
                    <option value="LB">LB - Light Blue</option>
                    <option value="NO">NO - None</option>
                    <option value="OR">OR - Orange</option>
                    <option value="PP">PP - Purple</option>
                    <option value="RD">RD - Red</option>
                </select>
                <button class=" btn">Edit Field</button>
            </div>

            <div style=" margin-top:5px"></div>

            <div class=" form-horizontal">

                Frequency Color: <select>
                    <option value="BL"> BL- Blue </option>
                    <option value="GR">GR - Green</option>
                    <option value="LB">LB - Light Blue</option>
                    <option value="NO">NO - None</option>
                    <option value="OR">OR - Orange</option>
                    <option value="PP">PP - Purple</option>
                    <option value="RD">RD - Red</option>
                </select>
                
                <button class=" btn">Edit Field</button>
            </div>

            <div style=" margin-top:5px"></div>

            <div class=" form-horizontal">

                Badge Use: <select>
                    <option value="PA"> PA- Participant </option>
                    <option value="AR">AR - Area Monitor</option>
                    <option value="CO">CO - Control</option>
                </select>


                <button class=" btn">Edit Field</button>
            </div>

      

        </div>
        <div id="buttonHolder">
            <input type="submit" id="btn_s" value="Submit" title="Submit">
        </div>
      
    </form>

</body>

</html>
